    "test:coverage": "umi-test --coverage",
    "sort": "sort-package-json"
  },
  "dependencies": {
    "@icon-park/react": "^1.3.3",
    "classnames": "^2.3.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.16",
    "dumi-theme-mobile": "^1.1.7",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^11.1.1",
    "prettier": "^2.2.1",
    "sort-package-json": "^1.50.0"
  }

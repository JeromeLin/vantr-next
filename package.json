{
  "name": "vantr",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "postinstall": "npm run sort && husky install",
    "start": "dumi dev",
    "build": "father-build",
    "build:docs": "dumi build",
    "lint": "npm run lint:script  && npm run lint:style && npm run lint:demo",
    "lint:script": "eslint . --ext '.js,.jsx,.ts,.tsx'",
    "lint:style": "stylelint '**/*.less' --syntax less",
    "lint:demo": "eslint **/*.md",
    "deploy": "npm run build:docs && gh-pages -d docs-dist",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "format": "pretty-quick",
    "test": "umi-test",
    "test:update": "umi-test -u",
    "test:coverage": "umi-test --coverage",
    "sort": "sort-package-json"
  },
  "dependencies": {
    "@icon-park/react": "^1.3.3",
    "classnames": "^2.3.1",
    "rc-field-form": "^1.21.1",
    "rc-motion": "^2.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.16",
    "dumi-theme-mobile": "^1.1.7",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-markdown": "^2.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "pinst": "^2.1.6",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "sort-package-json": "^1.50.0",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0"
  }
}
